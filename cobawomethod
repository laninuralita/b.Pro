public class cobawomethod
{
	public static void cetakgaris (){
	for (int i=0;i<35;i++)
		System.out.print("*");
		System.out.println();
		}
		
	public static void main(String []argv){
	cetakgaris();
	System.out.println("view data customer");
		cetakgaris();
		System.out.println("id\tnama\talamat");
	cetakgaris();
}
}
